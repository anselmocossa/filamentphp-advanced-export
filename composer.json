{
    "name": "filamentphp/advanced-export",
    "description": "Advanced export functionality for Filament resources with dynamic column selection, filtering, ordering, and background processing.",
    "keywords": [
        "laravel",
        "filament",
        "export",
        "excel",
        "xlsx",
        "dynamic-columns",
        "filament-plugin"
    ],
    "homepage": "https://github.com/filamentphp/advanced-export",
    "license": "MIT",
    "authors": [
        {
            "name": "Anselmo Kossa",
            "email": "anselmo.cossa@enh.co.mz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Filament\\AdvancedExport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filament\\AdvancedExport\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --no-coverage",
        "lint": "vendor/bin/pint",
        "lint-fix": "vendor/bin/pint --repair"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\AdvancedExport\\AdvancedExportServiceProvider"
            ],
            "aliases": {
                "AdvancedExport": "Filament\\AdvancedExport\\Facades\\AdvancedExport"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
